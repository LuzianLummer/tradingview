# Database
POSTGRES_USER=tradingview
POSTGRES_PASSWORD=tradingview
POSTGRES_DB=tradingview
POSTGRES_HOST=postgres

# Airflow
AIRFLOW__WEBSERVER__SECRET_KEY=change-me-please
AIRFLOW__DATABASE__SQL_ALCHEMY_CONN= postgresql+psycopg2://${POSTGRES_USER:-tradingview}:${POSTGRES_PASSWORD:-tradingview}@postgres/${POSTGRES_DB:-tradingview}
AIRFLOW__CORE__EXECUTOR= CeleryExecutor
AIRFLOW__CELERY__BROKER_URL= redis://redis:6379/0
AIRFLOW__CELERY__RESULT_BACKEND= db+postgresql://${POSTGRES_USER:-tradingview}:${POSTGRES_PASSWORD:-tradingview}@postgres/${POSTGRES_DB:-tradingview}
AIRFLOW__WEBSERVER__EXPOSE_CONFIG=True
AIRFLOW__LOGGING__REMOTE_LOGGING=False
AIRFLOW__CORE__EXECUTOR=CeleryExecutor
REDIS_HOST=redis
REDIS_PORT=6379
# Logging
LOG_LEVEL=INFO
LOG_FILE=app.log

